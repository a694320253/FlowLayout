<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FlowLayout">
        <attr name="flowlayout_line_vertical_gravity" format="enum">
            <enum name="top" value="0"/>
            <enum name="center_vertical" value="1"/>
